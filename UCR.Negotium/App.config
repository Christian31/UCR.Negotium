<?xml version="1.0" encoding="utf-8" ?>
<configuration>
    <startup useLegacyV2RuntimeActivationPolicy="true"> 
        <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.5.2" />
    </startup>
    <connectionStrings>
    <add name="db" connectionString="Data Source={AppDir}\Data\NegotiumDataBase.db; Version=3; Pooling=True; Max Pool Size=100;" />
  </connectionStrings>
  <!-- Installation -->
  <appSettings>
    <add key="Installed" value="True" />
 </appSettings>
</configuration>